@using laui.ViewModels;
@model SearchViewModel
@{
    ViewData["Title"] = "Trace transactions";
}

<div class="row">
    <div class="col-md-9">
        <h2>Trace transactions</h2>
    </div>
</div>
@{
    await Html.RenderPartialAsync("TransactionForm", Model);
}
@if(Model.Transactions != null && Model.Transactions.Count > 0)
{
    await Html.RenderPartialAsync("TransactionRecords", Model);
}