@using laui.ViewModels;
@model SearchViewModel
@{
    ViewData["Title"] = "Search";
}

<div class="row">
    <div class="col-md-9">
        <h2>Search</h2>
    </div>
</div>
@{
    await Html.RenderPartialAsync("Form", Model);
}
@if(Model.Transactions != null && Model.Transactions.Count > 0)
{
    await Html.RenderPartialAsync("Records", Model);
}